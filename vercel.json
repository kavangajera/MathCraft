{
    "version": 2,
    "builds": [
      {
        "src": "python-files/python-a21-ai.py",
        "use": "@vercel/python"
      },
      {
        "src": "backend/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/classify",
        "dest": "python-files/python-a21-ai.py"
      },
      {
        "src": "/api/(.*)",
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }